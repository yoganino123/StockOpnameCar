<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Stock Opname Car</title>
    <%- include('../addOn/link.ejs') %>

</head>
<body>
     <!-- Navbar -->
     <%- include('../addOn/navbar') %>

     <!-- Title -->
    <div class="container-fluid p-3 bg-black text-white text-center">
        <h1>All Report</h1>
    </div>
</br>

   

    <div class="container"col-12 my-2>
        <h3>All Report</h3>
    </br>
        <table class = "table table-bordered text-center container">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Car</th>
                    <th>Stock In</th>
                    <th>Stock Out</th>
                    <th>Date</th>
                </tr>
            </thead>
            <tbody>
                <% getDataReport.forEach(data => { %>
                    
                <tr>
                   
                    <td class="text-center"><%= data.id%> </td>
                    <td class="text-center"><%= data.car.name%></td>
                    <td class="text-center"><%= data.input && data.input.total ? data.input.total: "-" %></td>
                    <td class="text-center"><%= data.output && data.output.total ? data.output.total: "-" %></td>
                    
                 
                    <td class="text-center"><%= data.updatedAt%></td>
                </tr>
                <%})%>
            </tbody>

        </table>

    </div>


    
    
    

  <%- include('../addOn/script.ejs') %>
</body>
</html>